{
    "name": "electron-learning",
    "version": "1.0.0",
    "description": "Project for learnig Electron API",
    "main": "./dist/main.js",
    "scripts": {
        "build": "webpack --config webpack.config.js --mode=production",
        "build:main": "webpack --mode=development --config webpack.main.config.js",
        "build:renderer": "webpack --mode=development --config webpack.renderer.config.js",
        "build:all": "npm run build:main && npm run build:renderer",
        "webpack:serve": "webpack serve --config webpack.renderer.config.js --mode development",
        "start": "npm run build:main && concurrently \"npm run webpack:serve\" \"electron .\""
    },
    "prettier": {
        "trailingComma": "es5",
        "tabWidth": 4,
        "semi": false,
        "singleQuote": true,
        "printWidth": 100
    },
    "author": "Ilyacnt",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.22.5",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/preset-env": "^7.22.5",
        "@babel/preset-react": "^7.22.5",
        "@babel/preset-typescript": "^7.22.5",
        "@svgr/babel-preset": "^8.0.0",
        "@svgr/webpack": "^8.0.1",
        "@types/react": "^18.2.14",
        "@types/react-dom": "^18.2.6",
        "babel-loader": "^9.1.2",
        "clean-webpack-plugin": "^4.0.0",
        "concurrently": "^8.2.0",
        "css-loader": "^6.8.1",
        "electron": "^24.2.0",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.5.3",
        "image-webpack-loader": "^8.1.0",
        "postcss": "^8.4.24",
        "prettier-plugin-css-order": "^1.3.1",
        "style-loader": "^3.3.3",
        "ts-loader": "^9.4.3",
        "typescript-plugin-css-modules": "^5.0.1",
        "webpack": "^5.88.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^4.15.1"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^1.9.5",
        "child_process": "^1.0.2",
        "classnames": "^2.3.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-redux": "^8.1.1",
        "react-router-dom": "^6.14.0"
    }
}
